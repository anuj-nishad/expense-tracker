
generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "windows"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transactions{
  id          String @id @default(uuid())
  userId      String 
  title       String
  amount      Int 
  category    String
  created_at  DateTime @default(now())
}
